}
if (ip.dst == '192.168.1.67' && ip.proto == UDP && udp.src == 137) {
  msg("[morpheus]  host:192.168.1.67   [ -> ]  port:137 netbios ☠");
}
if (ip.dst == '192.168.1.67' && ip.proto == TCP && tcp.src == 139) {
  msg("[morpheus]  host:192.168.1.67   [ <- ]  port:139 netbios ☠");
}
if (ip.dst == '192.168.1.67' && ip.proto == TCP && tcp.src == 23) {
  msg("[morpheus]  host:192.168.1.67   [ <- ]  port:23 telnet ☠");
}
if (ip.dst == '192.168.1.67' && ip.proto == TCP && tcp.src == 22) {
  msg("[morpheus]  host:192.168.1.67   [ <- ]  port:22 ssh ☆");
}




